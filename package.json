{
  "name": "hello-world-vue",
  "displayName": "hello-world-vue",
  "version": "0.0.1",
  "description": "A hello world sample extension to demonstrate using Vue with the Webview UI Toolkit for VS Code.",
  "categories": [
    "Other"
  ],
  "main": "./out/src/extension.js",
  "engines": {
    "vscode": "^1.46.0"
  },
  "activationEvents": [    

  ],
  "contributes": {
    "commands": [
      {
        "command": "hello-world.showHelloWorld",
        "title": "Hello World (Vue): Show"
      }
    ]
  },
  "scripts": {
    "install:all": "pnpm install && cd webview-ui && pnpm install",
    "start:webview": "cd webview-ui && pnpm run start",
    "build:webview": "cd webview-ui && pnpm run build",
    "vscode:prepublish": "pnpm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "pnpm run compile && pnpm run lint",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  },
  "dependencies": {
    "@vscode/codicons": "^0.0.32"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.38.4",
    "@types/glob": "^7.2.0",
    "@types/node": "^12.20.55",
    "@types/vscode": "^1.77.0",
    "eslint": "^8.0.0",
    "glob": "^7.2.3",
    "lint-staged": "^13.2.1",
    "prettier": "^2.8.7",
    "simple-git-hooks": "^2.8.1",
    "typescript": "^4.9.5",
    "vscode-test": "^1.6.1"
  }
}
