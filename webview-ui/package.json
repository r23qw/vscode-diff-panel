{
  "name": "hello-world",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "concurrently \"pnpm run start\" \"pnpm run build:watch\" -n \"start,build\"",
    "start": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "build:watch": "vite build --watch",
    "typecheck": "vue-tsc --noEmit"
  },
  "dependencies": {
    "@arco-design/web-vue": "^2.45.1",
    "@monaco-editor/loader": "^1.3.3",
    "@vscode/codicons": "^0.0.32",
    "@vscode/webview-ui-toolkit": "^1.2.1",
    "@vueuse/core": "^10.1.0",
    "monaco-editor": "^0.37.1",
    "vue": "^3.2.33"
  },
  "devDependencies": {
    "@types/node": "^18.16.3",
    "@types/vscode-webview": "^1.57.0",
    "@unocss/transformer-directives": "^0.51.6",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/tsconfig": "^0.1.3",
    "concurrently": "^8.0.1",
    "typescript": "~4.6.3",
    "unocss": "^0.51.4",
    "unplugin-auto-import": "^0.15.3",
    "unplugin-vue-components": "^0.24.1",
    "vite": "^4.3.1",
    "vue-tsc": "^0.34.7"
  }
}
